
#include <stdio.h>
#include "sys/alt_irq.h"
#include "system.h"
#include "altera_avalon_pio_regs.h"

int background()
{
int j;
int x = 0;
int grainsize = 4;
int g_taskProcessed = 0;

for(j = 0; j < grainsize; j++)
{
g_taskProcessed++;
}
return x;
}

int main()
{
  printf("Hello from Nios II!\n");
  /*Set up EGM*/

  IOWR(EGM_BASE, 2, 1000);
  IOWR(EGM_BASE, 3, 500);
  int period = IORD(EGM_BASE, 2);
  int pulseWidth = IORD(EGM_BASE, 3);

  return 0;
}

